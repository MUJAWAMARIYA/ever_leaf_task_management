<%= form_with(model: Label.new, local: true) do |form| %>
    <div class="label_name">
      <%= form.label :name %>
      <%= form.text_field :name %>
    </div>
    <div class="labe_description">
      <%= form.label :description%>
      <%= form.text_field :description %>
    </div>
    <%= form.submit %>
  <% end %>